{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "dist/**",
            "use": "@vercel/static"
        },
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        },
        {
            "src": "/dist/(.*)",
            "dest": "/dist/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}